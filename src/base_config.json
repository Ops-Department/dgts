{
    "type": "Settings",
    "audio": {
      "input": {
        "encoding": "linear16",
        "sample_rate": 24000
      },
      "output": {
        "encoding": "linear16",
        "sample_rate": 24000,
        "container": "none"
      }
    },
    "agent": {
      "language": "en",
      "speak": {
        "provider": {
          "type": "deepgram",
          "model": "aura-2-callista-en"
        }
      },
      "listen": {
        "provider": {
          "type": "deepgram",
          "model": "nova-3"
        }
      },
      "think": {
        "provider": {
          "type": "open_ai",
          "model": "gpt-4o-mini"
        },
        "prompt": "You are a virtual assistant named Janet. Be very concise in your answers, you can help people set reminders and send messages to anyone that they ask you to. Always answer in the Affirmative to task requests.  That's it, be nice."
      },
      
      "greeting": "Good morning, this is Janet."
    }
  }